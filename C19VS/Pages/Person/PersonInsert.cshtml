@page
@model C19VS.Pages.Person.InsertModel
@{
}

<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="Person.medicare">medicare Number</label>
        <div class="col-sm-10">
            <input asp-for="Person.medicare" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.firstName">First Name</label>
        <div class="col-sm-10">
            <input asp-for="Person.firstName" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.lastName">Last Name</label>
        <div class="col-sm-10">
            <input asp-for="Person.lastName" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.dob">Date of Birth</label>
        <div class="col-sm-10">
            <input asp-for="Person.dob" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.telephone">telephone</label>
        <div class="col-sm-10">
            <input asp-for="Person.telephone" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.address">address</label>
        <div class="col-sm-10">
            <input asp-for="Person.address" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.city">city</label>
        <div class="col-sm-10">
            <input asp-for="Person.city" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.province">province</label>
        <div class="col-sm-10">
            <input asp-for="Person.province" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.postalCode">Postal Code</label>
        <div class="col-sm-10">
            <input asp-for="Person.postalCode" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.citizenship">citizenship</label>
        <div class="col-sm-10">
            <input asp-for="Person.citizenship" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.email">email</label>
        <div class="col-sm-10">
            <input asp-for="Person.email" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.infected">infected</label>
        <div class="col-sm-10">
            <input asp-for="Person.infected" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.ageGroup">ageGroup</label>
        <div class="col-sm-10">
            <input asp-for="Person.ageGroup" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Person.SSN">SSN</label>
        <div class="col-sm-10">
            <input asp-for="Person.SSN" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-page="/Person/PersonIndex" class="btn btn-primary">Cancel</a>
        </div>
    </div>
</form>