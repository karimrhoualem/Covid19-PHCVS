@page
@model C19VS.Pages.Dose.InsertModel
@{
}

<h1>Inserting a new Doset</h1>

<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="Dose.SSN">SSN</label>
        <div class="col-sm-10">
            <input asp-for="Dose.SSN" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Dose.doseNumber">Dose Number</label>
        <div class="col-sm-10">
            <input asp-for="Dose.doseNumber" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Dose.doseDate">Dose Date</label>
        <div class="col-sm-10">
            <input asp-for="Dose.doseDate" type="date" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Dose.vacTypeID">Vaccine ID</label>
        <div class="col-sm-10">
            <input asp-for="Dose.vacTypeID" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Dose.facilityID">FacilityID</label>
        <div class="col-sm-10">
            <input asp-for="Dose.facilityID" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Dose.EID">Employee ID</label>
        <div class="col-sm-10">
            <input asp-for="Dose.EID" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-page="/Dose/DoseIndex" class="btn btn-primary">Cancel</a>
        </div>
    </div>
</form>