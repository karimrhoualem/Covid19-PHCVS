@page
@model C19VS.Pages.Employees.InsertModel
@{
}

<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="Employees.SSN">SSN</label>
        <div class="col-sm-10">
            <input asp-for="Employees.SSN" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.EID">Employee ID</label>
        <div class="col-sm-10">
            <input asp-for="Employees.EID" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.firstName">First Name</label>
        <div class="col-sm-10">
            <input asp-for="Employees.firstName" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.lastName">Last Name</label>
        <div class="col-sm-10">
            <input asp-for="Employees.lastName" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.dob">Date of Birth</label>
        <div class="col-sm-10">
            <input asp-for="Employees.dob" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.telephone">Telephone</label>
        <div class="col-sm-10">
            <input asp-for="Employees.telephone" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.address">Address</label>
        <div class="col-sm-10">
            <input asp-for="Employees.address" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.city">City</label>
        <div class="col-sm-10">
            <input asp-for="Employees.city" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.province">Province</label>
        <div class="col-sm-10">
            <input asp-for="Employees.province" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.postalCode">Postal Code</label>
        <div class="col-sm-10">
            <input asp-for="Employees.postalCode" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.citizenship">Citizenship</label>
        <div class="col-sm-10">
            <input asp-for="Employees.citizenship" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.email">Email</label>
        <div class="col-sm-10">
            <input asp-for="Employees.email" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.infected">Infected</label>
        <div class="col-sm-10">
            <input asp-for="Employees.infected" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Employees.ageGroup">Age Group</label>
        <div class="col-sm-10">
            <input asp-for="Employees.ageGroup" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-page="/Employees/EmployeesIndex" class="btn btn-primary">Cancel</a>
        </div>
    </div>
</form>