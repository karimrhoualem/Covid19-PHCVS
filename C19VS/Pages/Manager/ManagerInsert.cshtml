@page
@model C19VS.Pages.Manager.InsertModel
@{
}

<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="Manager.SSN">SSN</label>
        <div class="col-sm-10">
            <input asp-for="Manager.SSN" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.EID">Employee ID</label>
        <div class="col-sm-10">
            <input asp-for="Manager.EID" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.facilityID">Facility ID</label>
        <div class="col-sm-10">
            <input asp-for="Manager.facilityID" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.firstName">First Name</label>
        <div class="col-sm-10">
            <input asp-for="Manager.firstName" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.lastName">Last Name</label>
        <div class="col-sm-10">
            <input asp-for="Manager.lastName" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.dob">Date of Birth</label>
        <div class="col-sm-10">
            <input asp-for="Manager.dob" type="date" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.telephone">Telephone</label>
        <div class="col-sm-10">
            <input asp-for="Manager.telephone" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.address">Address</label>
        <div class="col-sm-10">
            <input asp-for="Manager.address" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.city">City</label>
        <div class="col-sm-10">
            <input asp-for="Manager.city" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.province">Province</label>
        <div class="col-sm-10">
            <input asp-for="Manager.province" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.postalCode">Postal Code</label>
        <div class="col-sm-10">
            <input asp-for="Manager.postalCode" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.citizenship">Citizenship</label>
        <div class="col-sm-10">
            <input asp-for="Manager.citizenship" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.email">Email</label>
        <div class="col-sm-10">
            <input asp-for="Manager.email" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.infected">Infected</label>
        <div class="col-sm-10">
            <input asp-for="Manager.infected" />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Manager.ageGroup">Age Group</label>
        <div class="col-sm-10">
            <input asp-for="Manager.ageGroup" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-page="/Manager/ManagerIndex" class="btn btn-primary">Cancel</a>
        </div>
    </div>
</form>