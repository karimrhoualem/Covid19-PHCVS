@page
@model C19VS.Pages.Infection.InsertModel
@{
}


<form method="post" class="mt-3">

    <div class="form-group row">
        <label asp-for="Infection.SSN">SSN</label>
        <div class="col-sm-10">
            <input asp-for="Infection.SSN"  />
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Infection.infectionDate">Infection Date</label>
        <div class="col-sm-10">
            <input asp-for="Infection.infectionDate"/>
        </div>
    </div>

    <div class="form-group row">
        <label asp-for="Infection.variantID">Variant ID</label>
        <div class="col-sm-10">
            <input asp-for="Infection.variantID" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-page="/Infection/InfectionIndex" class="btn btn-primary">Cancel</a>
        </div>
    </div>
</form>