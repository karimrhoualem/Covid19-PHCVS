@page
@model C19VS.Pages.Shipment.ShipmentInsertModel
@{
}

<h1>Inserting a new Shipment</h1>

<form method="post" class="mt-3">
    <div class="form-group">
        <label asp-for="Shipment.shipmentID">shipmentID</label>
        <div class="col-sm-10">
            <input asp-for="Shipment.shipmentID"/>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Shipment.VacTypeID">VacTypeID</label>
        <div class="col-sm-10">
            <input asp-for="Shipment.VacTypeID"/>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Shipment.facilityID">facilityID</label>
        <div class="col-sm-10">
            <input asp-for="Shipment.facilityID"/>
        </div>
    </div>

    <div class="form-group ">
        <label asp-for="Shipment.receptionDate">receptionDate</label>
        <div class="col-sm-10">
            <input asp-for="Shipment.receptionDate" type="date"/>
        </div>
    </div>

    <div class="form-group ">
        <label asp-for="Shipment.numberVaccine">numberVaccine</label>
        <div class="col-sm-10">
            <input asp-for="Shipment.numberVaccine"/>
        </div>
    </div>

    <div class="form-group ">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Save</button>
            <a asp-page="/Shipment/ShipmentIndex" class="btn btn-primary">Cancel</a>
        </div>
    </div>
</form>